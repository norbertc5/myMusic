<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyMusic.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyMusic"
    Title="MyMusic"
    BackgroundColor="#6EC8C9"
    >

    <Shell.TitleView>
        <Label Text="MyMusic" FontSize="35" FontFamily="HoltwoodOneSC" FontAttributes="Bold" Padding="0, 0, 0, 0" />
    </Shell.TitleView>

    <ShellContent
        Title="MyMusic"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"
        />

</Shell>
